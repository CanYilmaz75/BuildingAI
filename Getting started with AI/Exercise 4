
import math
import random
import numpy as np
import io
from io import StringIO
import random

def main():
    rNumber = random.random()
    if(rNumber < 0.8):
        favourite = 'dogs'
    elif(rNumber < 0.9):
        favourite = 'cats'
    else:
         favourite = "bats"  # change this  

    print("I love " + favourite) 


main()
